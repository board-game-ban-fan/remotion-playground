{
  "name": "react-scss-modules-storybook-library-template",
  "version": "0.2.2",
  "homepage": "https://gorestarry.github.io/react-scss-modules-storybook-library-template/",
  "author": {
    "name": "Gore Wang",
    "email": "sunrise91.t3@gmail.com",
    "url": "https://gore.wang"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "source": "src/lib.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf docs && npm-run-all --parallel build-lib build-app build-storybook",
    "start": "react-scripts start",
    "build-lib": "rollup -c",
    "build-lib-watch": "rollup -c -w",
    "build-app": "react-scripts build && mv -f build/* docs",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "npm-run-all --parallel build-lib-watch storybook",
    "storybook": "start-storybook -p 6006 -s public --no-dll",
    "build-storybook": "build-storybook -o ./docs/storybook -s public --no-dll",
    "minor": "npm version minor --no-git-tag-version",
    "patch": "npm version patch --no-git-tag-version"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@storybook/addon-storysource": "^6.0.28",
    "@storybook/addon-knobs": "^6.0.28",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/node-logger": "^6.0.28",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.28",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "babel-loader": "^8.1.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-is": "^17.0.1",
    "react-scripts": "4.0.0",
    "rollup": "^2.33.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "web-vitals": "^0.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
